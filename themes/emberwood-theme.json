{
  "name": "Emberwood",
  "type": "dark",
  "colors": {
    "editor.background": "#171C1A",
    "editor.foreground": "#FFB86C",
    "editorCursor.foreground": "#FF6B2D",
    "editor.lineHighlightBackground": "#222925",
    "editor.selectionBackground": "#2A363188",
    "editor.selectionHighlightBackground": "#2A363144",
    "editorIndentGuide.background1": "#2A3631",
    "editorIndentGuide.activeBackground1": "#5A6964",
    "editorWhitespace.foreground": "#33333366",
    "editorLineNumber.foreground": "#5A6964",
    "editorLineNumber.activeForeground": "#FF6B2D",
    "sideBar.background": "#141917",
    "sideBar.foreground": "#E0E5E3",
    "activityBar.background": "#141917",
    "activityBar.foreground": "#E0E5E3",
    "statusBar.background": "#141917",
    "statusBar.foreground": "#E0E5E3",
    "statusBarItem.prominentForeground": "#86cea4",
    "titleBar.activeBackground": "#141917",
    "titleBar.activeForeground": "#E0E5E3",
    "tab.activeBackground": "#171C1A",
    "tab.inactiveBackground": "#141917",
    "tab.activeForeground": "#FF6B2D",
    "tab.inactiveForeground": "#5A6964",
    "editorGroupHeader.tabsBackground": "#141917",
    "icon.foreground": "#E0E5E3",
    "toolbar.hoverBackground": "#222925",
    "editorGroup.border": "#141917",
    "input.background": "#222925",
    "input.foreground": "#E0E5E3",
    "input.border": "#5A6964",
    "input.placeholderForeground": "#5A6964",
    "button.background": "#86cea4",
    "button.foreground": "#141917",
    "button.hoverBackground": "#a1d9b9",
    "dropdown.background": "#141917",
    "dropdown.border": "#5A6964",
    "editorBracketHighlight.foreground1": "#d3b5d6",
    "editorBracketHighlight.foreground2": "#FFB86C",
    "editorBracketHighlight.foreground3": "#86cea4",
    "editorBracketHighlight.foreground4": "#f5e3a6",
    "editorBracketHighlight.foreground5": "#a1d9b9",
    "editorBracketHighlight.foreground6": "#E0E5E3",
    "editorBracketHighlight.unexpectedBracket.foreground": "#FF5572",
    "gitDecoration.modifiedResourceForeground": "#f5e3a6",
    "gitDecoration.deletedResourceForeground": "#FF5572",
    "gitDecoration.untrackedResourceForeground": "#86cea4",
    "gitDecoration.conflictingResourceForeground": "#FF6B2D",
    "gitDecoration.submoduleResourceForeground": "#8D9E97",
    "terminal.background": "#171C1A",
    "terminal.foreground": "#E0E5E3",
    "terminalCursor.background": "#FF6B2D",
    "terminalCursor.foreground": "#171C1A",
    "terminal.ansiBlack": "#141917",
    "terminal.ansiBrightBlack": "#5A6964",
    "terminal.ansiRed": "#FF5572",
    "terminal.ansiBrightRed": "#FF8095",
    "terminal.ansiGreen": "#86cea4",
    "terminal.ansiBrightGreen": "#a1d9b9",
    "terminal.ansiYellow": "#f5e3a6",
    "terminal.ansiBrightYellow": "#f8eebd",
    "terminal.ansiBlue": "#8D9E97",
    "terminal.ansiBrightBlue": "#a3b1ab",
    "terminal.ansiMagenta": "#d3b5d6",
    "terminal.ansiBrightMagenta": "#e0cce2",
    "terminal.ansiCyan": "#a1d9b9",
    "terminal.ansiBrightCyan": "#b3e6d1",
    "terminal.ansiWhite": "#E0E5E3",
    "terminal.ansiBrightWhite": "#f1f6f8"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#5A6964", "fontStyle": "italic" }
    },
    {
      "name": "punctuation and operators",
      "scope": ["punctuation", "keyword.operator"],
      "settings": { "foreground": "#E0E5E3" }
    },

    {
      "name": "Keywords and Control Structures",
      "scope": ["keyword.control", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#86cea4" }
    },
    {
      "name": "functions and Methods",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#a3b1ab" }
    },
    {
      "name": "variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter",
        "entity.name.variable"
      ],
      "settings": { "foreground": "#d3b5d6" }
    },
    {
      "name": "constants",
      "scope": [
        "constant",
        "constant.other",
        "constant.numeric",
        "constant.language.boolean",
        "support.constant",
        "string"
      ],
      "settings": { "foreground": "#FF6B2D" }
    },
    {
      "name": "Properties and Escape Characters",
      "scope": ["variable.other.property", "constant.character.escape"],
      "settings": { "foreground": "#FF8095" }
    },
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.class",
        "support.type"
      ],
      "settings": { "foreground": "#FF8095" }
    },
    {
      "name": "Decorators",
      "scope": ["entity.name.decorator", "meta.decorator"],
      "settings": { "foreground": "#FFB86C", "fontStyle": "italic" }
    },
    {
      "name": "HTML/JSX Tags",
      "scope": "entity.name.tag",
      "settings": { "foreground": "#86cea4" }
    },
    {
      "name": "HTML/JSX Attributes",
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#FF8095" }
    },
    {
      "name": "Markdown: Headings",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": { "foreground": "#FF6B2D", "fontStyle": "bold" }
    },
    {
      "name": "Markdown: Bold",
      "scope": "markup.bold",
      "settings": { "fontStyle": "bold" }
    },
    {
      "name": "Markdown: Italic",
      "scope": "markup.italic",
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Invalid or Deprecated Code",
      "scope": "invalid",
      "settings": { "foreground": "#FF5572", "fontStyle": "underline" }
    },
    {
      "name": "Deprecated Code",
      "scope": "invalid.deprecated",
      "settings": { "foreground": "#FF5572", "fontStyle": "strikethrough" }
    }
  ]
}
